stock FormatarHorario()
{
	new
		Tempo[64]
	;
	new
		Hora, Minuto
	;
	gettime(Hora, Minuto);
	Hora += FUSORARIO;
    format(Tempo, sizeof(Tempo), "%02d:%02d", Hora, Minuto);
	return Tempo;
}

stock FormatarData()
{
    new
		data[64], ano, mes, dia
	;
	getdate(ano, mes, dia);
	format(data, sizeof(data), "%02d/%02d/%02d", dia, mes, ano);
	TextDrawSetString(Data, data);
	return data;
}

