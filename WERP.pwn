	   				    AttachTrailerToVehicle(vid, GetPlayerVehicleID(playerid));
	   				    SendClientMessage(playerid, COR_AMARELO,"Veículo guinchado com sucesso!");
	   				}
	       		}
				if(!Found)
				{
				    SendClientMessage(playerid, COR_ERRO, "Não à nenhum veículo para você guinchar!");
				}
			}
		}
		return 1;
	}
