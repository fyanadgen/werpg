		if(Distance <= 4.0 && playerState == 1 || Distance > 4.0 && Distance <= 10.0 && playerState == 2 && GetPlayerVehicleID(playerid) == 407 || FireAtual[playerid] == 0)
		{
      		KillTimer(FireTimerP[playerid]);
