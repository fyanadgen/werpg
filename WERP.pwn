					TextDrawShowForPlayer(i, IMG_Morreu);
				}
				else
				{
				    TextDrawHideForPlayer(i, MSG_Matou[i]);
				    TextDrawHideForPlayer(i, IMG_Matou);
					TextDrawHideForPlayer(i, IMG_Morreu);
				}
				if(TextFavela[i] == false)
				{
					TextFavela[i] = true;
