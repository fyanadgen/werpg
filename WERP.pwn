		SalvarFile(i);
		SalvarFSS(i);
	}
	DestruirTextsDraws();

	for(new i; i < 5; ++i)
	{
		TextDrawDestroy(dmColoText[i]);
	}
	TextDrawDestroy(dm_text_Top5[0]);
	TextDrawDestroy(dm_text_Top5[1]);
	TextDrawDestroy(dmiTimer[0]);
